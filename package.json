{
  "name": "docusaurus-book-with-ai-agents",
  "version": "1.0.0",
  "description": "Docusaurus book app with AI agents, authentication, personalization and translation",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "start:agents": "npm-run-all --parallel start:summarizer start:coderunner start:translator",
    "start:summarizer": "cd agents/summarizer && python -m uvicorn simple_main:app --reload --port 8001",
    "start:coderunner": "cd agents/code_runner && python -m uvicorn main:app --reload --port 8002",
    "start:translator": "cd agents/translator && python -m uvicorn main:app --reload --port 8003",
    "start:auth": "cd auth && npx ts-node server.ts",
    "dev": "npm-run-all --parallel start:agents start:auth"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "http-proxy-middleware": "^3.0.5",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@google/generative-ai": "^0.17.1",
    "@mdx-js/react": "^3.0.0",
    "better-auth": "^0.2.1",
    "clsx": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "prism-react-renderer": "^2.3.0",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.1.0",
    "remark-gfm": "^4.0.1"
  },
  "keywords": [
    "docusaurus",
    "ai-agents",
    "authentication",
    "translation",
    "personalization"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "24.x"
  }
}
